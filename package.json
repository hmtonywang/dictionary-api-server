{
  "name": "y-dictionary-server",
  "version": "0.1.0",
  "description": "A service to look up vocabularies from the Yahoo dictionary.",
  "author": "hm.tony.wang@gmail.com",
  "main": "server.js",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "start": {
      "command": "node server.js",
      "env": {
        "NODE_ENV": "production",
        "TZ": "Asia/Taipei"
      }
    },
    "dev": {
      "command": "node --watch server.js",
      "env": {
        "NODE_ENV": "development",
        "TZ": "Asia/Taipei"
      }
    },
    "test": {
      "command": "",
      "env": {
        "TZ": "Asia/Taipei"
      }
    }
  },
  "dependencies": {
    "@godaddy/terminus": "^4.11.2",
    "axios": "^1.3.2",
    "better-npm-run": "^0.1.1",
    "bunyan": "^1.8.15",
    "cheerio": "^1.0.0-rc.12",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.3",
    "http-status": "^1.6.2",
    "moment": "^2.29.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "swagger-jsdoc": "^6.2.8"
  }
}
